<view><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><view data-event-opts="{{[['tap',[['select',[index]]]]]}}" class="{{(('target')+' '+(target.selected?'on':''))}}" onTap="__e" a:for="{{orderbyList}}" a:for-item="target" a:for-index="index" a:key="index">{{''+target.text+''}}<block a:if="{{target.orderbyicon}}"><view class="{{(('icon')+' '+target.orderbyicon[target.orderby])}}"></view></block></view></view><view class="place"></view><view class="goods-list"><view class="product-list"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','goods_id',goods.goods_id]]]]]]]}}" class="product" onTap="__e" a:for="{{goodsList}}" a:for-item="goods" a:for-index="__i0__" a:key="goods_id"><image mode="widthFix" src="{{goods.img}}"></image><view class="name">{{goods.name}}</view><view class="info"><view class="price">{{goods.price}}</view><view class="slogan">{{goods.slogan}}</view></view></view></view><view class="loading-text">{{loadingText}}</view></view></view>
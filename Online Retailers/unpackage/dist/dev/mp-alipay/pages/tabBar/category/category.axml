<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><view class="addr"><view class="icon location"></view>{{city}}</view><view class="input-box"><input placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" onTap="__e"/><view class="icon search"></view></view><view class="icon-btn"><view data-event-opts="{{[['tap',[['toMsg',['$event']]]]]}}" class="icon tongzhi" onTap="__e"></view></view></view><view class="place"></view><view class="category-list"><scroll-view class="left" scroll-y="true"><view data-event-opts="{{[['tap',[['showCategory',[index]]]]]}}" class="{{(('row')+' '+(index==showCategoryIndex?'on':''))}}" onTap="__e" a:for="{{categoryList}}" a:for-item="category" a:for-index="index" a:key="id"><view class="text"><view class="block"></view>{{''+category.title+''}}</view></view></scroll-view><scroll-view class="right" scroll-y="true"><view hidden="{{!(index==showCategoryIndex)}}" class="category" a:for="{{categoryList}}" a:for-item="category" a:for-index="index" a:key="id"><view class="banner"><image src="{{category.banner}}"></image></view><view class="list"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['categoryList','id',category.id],['list','',i]]]]]]]}}" class="box" onTap="__e" a:for="{{category.list}}" a:for-item="box" a:for-index="i" a:key="i"><image src="{{'/static/img/category/list/'+box.img}}"></image><view class="text">{{box.name}}</view></view></view></view></scroll-view></view></view>
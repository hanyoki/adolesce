<view><block a:if="{{showHeader}}"><view class="status" style="{{'position:'+(headerPosition)+';'+('top:'+(statusTop)+';')+('opacity:'+(afterHeaderOpacity)+';')}}"></view></block><block a:if="{{showHeader}}"><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')+('opacity:'+(afterHeaderOpacity)+';')}}"><view class="addr"><view class="icon location"></view>{{''+city+''}}</view><view class="input-box"><input placeholder="默认关键字" placeholder-style="color:#c0c0c0;" data-event-opts="{{[['tap',[['toSearch']]]]}}" onTap="__e"/><view class="icon search"></view></view><view class="icon-btn"><view class="icon yuyin-home"></view><view data-event-opts="{{[['tap',[['toMsg',['$event']]]]]}}" class="icon tongzhi" onTap="__e"></view></view></view></block><block a:if="{{showHeader}}"><view class="place"></view></block><view class="swiper"><view class="swiper-box"><swiper circular="true" autoplay="true" data-event-opts="{{[['change',[['swiperChange',['$event']]]]]}}" onChange="__e"><swiper-item a:for="{{swiperList}}" a:for-item="swiper" a:for-index="__i0__" a:key="id"><image src="{{swiper.img}}" data-event-opts="{{[['tap',[['toSwiper',['$0'],[[['swiperList','id',swiper.id]]]]]]]}}" onTap="__e"></image></swiper-item></swiper><view class="indicator"><view class="{{(('dots')+' '+(currentSwiper>=index?'on':''))}}" a:for="{{swiperList}}" a:for-item="swiper" a:for-index="index" a:key="index"></view></view></view></view><view class="category-list"><view data-event-opts="{{[['tap',[['toCategory',['$0'],[[['categoryList','',index]]]]]]]}}" class="category" onTap="__e" a:for="{{categoryList}}" a:for-item="row" a:for-index="index" a:key="index"><view class="img"><image src="{{row.img}}"></image></view><view class="text">{{row.name}}</view></view></view><view class="banner"><image src="/static/img/banner.jpg"></image></view><view class="promotion"><view class="text">优惠专区</view><view class="list"><view data-event-opts="{{[['tap',[['toPromotion',['$0'],[[['Promotion','',index]]]]]]]}}" class="column" onTap="__e" a:for="{{Promotion}}" a:for-item="row" a:for-index="index" a:key="index"><view class="top"><view class="title">{{row.title}}</view><block a:if="{{row.countdown}}"><view class="countdown"><view>{{row.countdown.h}}</view>:<view>{{row.countdown.m}}</view>:<view>{{row.countdown.s}}</view></view></block></view><view class="left"><view class="ad">{{row.ad}}</view><view class="into">点击进入</view></view><view class="right"><image src="{{row.img}}"></image></view></view></view></view><view class="goods-list"><view class="title"><image src="/static/img/hua.png"></image>猜你喜欢<image src="/static/img/hua.png"></image></view><view class="product-list"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['productList','goods_id',product.goods_id]]]]]]]}}" class="product" onTap="__e" a:for="{{productList}}" a:for-item="product" a:for-index="__i1__" a:key="goods_id"><image mode="widthFix" src="{{product.img}}"></image><view class="name">{{product.name}}</view><view class="info"><view class="price">{{product.price}}</view><view class="slogan">{{product.slogan}}</view></view></view></view><view class="loading-text">{{loadingText}}</view></view></view>
<view><view class="content"><view class="list"><view data-event-opts="{{[['tap',[['select',['$0'],[[['addressList','',index]]]]]]]}}" class="row" onTap="__e" a:for="{{addressList}}" a:for-item="row" a:for-index="index" a:key="index"><view class="left"><view class="head">{{''+row.head+''}}</view></view><view class="center"><view class="name-tel"><view class="name">{{row.name}}</view><view class="tel">{{row.tel}}</view><block a:if="{{row.isDefault}}"><view class="default">默认</view></block></view><view class="address">{{''+row.address.region.label+" "+row.address.detailed+''}}</view></view><view class="right"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['addressList','',index]]]]]]]}}" class="icon bianji" catchTap="__e"></view></view></view></view></view><view class="add"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="btn" onTap="__e"><view class="icon tianjia"></view>新增地址</view></view></view>
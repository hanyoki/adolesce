<view><view class="content"><view class="list"><block wx:for="{{addressList}}" wx:for-item="row" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',['$0'],[[['addressList','',index]]]]]]]}}" class="row" bindtap="__e"><view class="left"><view class="head">{{''+row.head+''}}</view></view><view class="center"><view class="name-tel"><view class="name">{{row.name}}</view><view class="tel">{{row.tel}}</view><block wx:if="{{row.isDefault}}"><view class="default">默认</view></block></view><view class="address">{{''+row.address.region.label+" "+row.address.detailed+''}}</view></view><view class="right"><view data-event-opts="{{[['tap',[['edit',['$0'],[[['addressList','',index]]]]]]]}}" class="icon bianji" catchtap="__e"></view></view></view></block></view></view><view class="add"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="btn" bindtap="__e"><view class="icon tianjia"></view>新增地址</view></view></view>
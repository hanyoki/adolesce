view {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.add {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.add .btn {
